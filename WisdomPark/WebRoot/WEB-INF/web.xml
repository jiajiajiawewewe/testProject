<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>login</servlet-name>
    <servlet-class>com.wisdom.controller.login</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.wisdom.controller.LogoutServlet</servlet-class>
  </servlet>



 <servlet>
    <servlet-name>Publish</servlet-name>
    <jsp-file>/WEB-INF/jsp/publish.jsp</jsp-file>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>AddArticleReplyAdmin</servlet-name>
    <servlet-class>com.wisdom.controller.AddArticleReplyAdmin</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>AddAdminReply2</servlet-name>
    <servlet-class>com.wisdom.controller.AddAdminReply2</servlet-class>
  </servlet>


  
 
  <servlet-mapping>
    <servlet-name>Publish</servlet-name>
    <url-pattern>/publish</url-pattern>
  </servlet-mapping>

    
  <welcome-file-list>
    <welcome-file>ListAtricle</welcome-file>
  </welcome-file-list>
  


  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddArticleReplyAdmin</servlet-name>
    <url-pattern>/servlet/AddArticleReplyAdmin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddAdminReply2</servlet-name>
    <url-pattern>/servlet/AddAdminReply2</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  <welcome-file>home.jsp</welcome-file>
  </welcome-file-list>

</web-app>


  
  
